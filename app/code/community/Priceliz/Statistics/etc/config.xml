<?xml version="1.0"?>
<config>
    <modules>
        <priceliz_statistics>
            <version>1.0.0</version>
            <depends>
                <!-- no dependencies -->
            </depends>
        </priceliz_statistics>
    </modules>
    <global>
        <models/>
        <blocks/>
        <resources/>
        <extraconfig />
        <helpers>
            <statistics>
                <class>Priceliz_Statistics_Helper</class>
            </statistics>
        </helpers>
    </global>
    <admin>
        <routers>
            <priceliz_statistics>
                <use>admin</use>
                <args>
                    <module>Priceliz_Statistics</module>
                    <frontName>statistics</frontName>
                </args>
            </priceliz_statistics>
        </routers>
    </admin>
</config>
